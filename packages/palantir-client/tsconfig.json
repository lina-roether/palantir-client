{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"rootDir": "src",
		"outDir": "dist",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"importHelpers": true,
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true
	},
}
