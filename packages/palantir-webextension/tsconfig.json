{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"target": "ES2020",
		"module": "ES2020",
		"moduleResolution": "Bundler",
		"verbatimModuleSyntax": true,
		"strict": true,
		"skipLibCheck": true,
		"importHelpers": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true
	},
}
